    "version": "0.2.0",
    "env": {
        "CI": 1
    },
    "configurations": [
        {
            "name": "PowerShell Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${cwd}"
        },
        {
            "name": "PowerShell: Launch Script",
            "type": "PowerShell",
            "request": "launch",
            "script": "\"${workspaceFolder}/.github/scripts/Build-Windows.ps1\"",
            "cwd": "${workspaceFolder}/.github/scripts"
        }
    ]
}
