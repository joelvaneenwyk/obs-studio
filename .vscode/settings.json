{
    "cmake.environment": {
        "ENABLE_PIPEWIRE": "OFF",
        "ENABLE_ALSA": "OFF"
    },
    "cmake.buildEnvironment": {
        "ENABLE_PIPEWIRE": "OFF",
        "ENABLE_ALSA": "OFF"
    },
    "[json]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.experimentalWhitespaceRendering": "font",
    },
    "[jsonc]": {
        "editor.tabSize": 2,
        "editor.experimentalWhitespaceRendering": "font",
    },
    "cmake.buildDirectory": "${workspaceFolder}/.build/vscode",
    "cmake.configureEnvironment": {
        "ENABLE_PIPEWIRE": "OFF",
        "ENABLE_ALSA": "OFF"
    },
    "terminal.integrated.env.linux": {
        "DISABLE_PIPEWIRE": "TRUE"
    },
    "editor.detectIndentation": false,
    "cmake.automaticReconfigure": true,
    "cmake.cmakeCommunicationMode": "automatic",
    "cmake.configureOnEdit": true,
    "cmake.configureOnOpen": true,
    "cmake.autoSelectActiveFolder": true,
    "cmake.enableTraceLogging": true,
    "cmake.revealLog": "always",
    "cmake.allowCommentsInPresetsFile": true,
    "cmake.configureArgs": [
        "--debug-output"
    ],
    "cmake.buildTask": true,
    "cmake.buildToolArgs": [
        "-DENABLE_ALSA=OFF"
    ],
    "cmake.buildArgs": [
        "-DENABLE_ALSA=OFF"
    ],
    "cmake.configureSettings": {
        "ENABLE_ALSA": "OFF",
        "ENABLE_PIPEWIRE": "OFF"
    },
    "json.format.keepLines": true,
    "json.trace.server": "messages"
}
